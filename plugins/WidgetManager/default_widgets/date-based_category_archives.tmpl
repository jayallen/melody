<MTIfArchiveTypeEnabled archive_type="Category-Yearly">
    <div class="widget-categories widget">
        <h3 class="widget-header">Category Yearly Archives</h3>
        <div class="widget-content">
            <MTArchiveList archive_type="Category-Yearly">
                <MTArchiveListHeader>
                    <ul class="widget-list">
                </MTArchiveListHeader>
                <li class="widget-list-item"><a href="<$MTArchiveLink$>"><$MTArchiveTitle$> (<$MTArchiveCount$>)</a></li>
                <MTArchiveListFooter>
                    </ul>
                </MTArchiveListFooter>
            </MTArchiveList>
        </div>
    </div>
</MTIfArchiveTypeEnabled>

