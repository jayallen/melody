CREATE TABLE `mt_stats` (
  `stats_id` int(10) unsigned NOT NULL auto_increment,
  `stats_ip` tinytext NOT NULL,
  `stats_created_on` datetime NOT NULL default '0000-00-00 00:00:00',
  `stats_created_by` text,
  `stats_modified_on` datetime NOT NULL default '0000-00-00 00:00:00',
  `stats_modified_by` text,
  `stats_referrer` text NOT NULL,
  `stats_blog_id` tinyint(3) unsigned NOT NULL default '0',
  `stats_url` text NOT NULL,
  PRIMARY KEY  (`stats_id`),
  KEY `stats_blog_id` (`stats_blog_id`),
  KEY `stats_created_on` (`stats_created_on`)
) TYPE=MyISAM;

CREATE TABLE `mt_statwatchconfig` (
  `statwatchconfig_id` int(10) unsigned NOT NULL auto_increment,
  `statwatchconfig_created_on` datetime NOT NULL default '0000-00-00 00:00:00',
  `statwatchconfig_created_by` text,
  `statwatchconfig_modified_on` datetime NOT NULL default '0000-00-00 00:00:00',
  `statwatchconfig_modified_by` text,
  `statwatchconfig_blog_id` tinyint(3) unsigned NOT NULL default '0',
  `statwatchconfig_last_compile` datetime default '0000-00-00 00:00:00',
  `statwatchconfig_average_hits` mediumint(8) unsigned NOT NULL default '0',
  `statwatchconfig_average_hits_count` int(10) unsigned NOT NULL default '0',
  `statwatchconfig_average_uniques` mediumint(8) unsigned NOT NULL default '0',
  `statwatchconfig_average_uniques_count` int(10) unsigned NOT NULL default '0',
  `statwatchconfig_average_hour` mediumint(8) unsigned NOT NULL default '0',
  `statwatchconfig_average_hour_count` int(10) unsigned NOT NULL default '0',
  `statwatchconfig_average_feed` mediumint(8) unsigned NOT NULL default '0',
  `statwatchconfig_average_feed_count` int(10) unsigned NOT NULL default '0',
  `statwatchconfig_feed_url` text,
  PRIMARY KEY  (`statwatchconfig_id`),
  KEY `statwatchconfig_blog_id` (`statwatchconfig_blog_id`),
  KEY `statwatchconfig_created_on` (`statwatchconfig_created_on`)
) TYPE=MyISAM;
        