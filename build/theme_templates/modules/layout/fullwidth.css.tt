[%# Layout alterations for wide themes like Fleur, Flowers, and Trees. %]

#container-inner {
    margin-right: 0;
    margin-left: 0;
}

.layout-wtt #container-inner,
.layout-twt #container-inner,
.layout-wt #container-inner,
.layout-tw #container-inner {
    width: 100%;
}

#header-inner,
#content-inner,
#footer-inner {
    position: relative;
    width: 940px;
    margin: 0 auto;
}
