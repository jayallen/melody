<mt:setvarblock name="action_buttons">
    <a href="javascript:void(0)"
        onclick="doRemoveItems(getByID('<$mt:var name="object_type"$>-listing-form'), '<__trans phrase="cache" escape="js">', '<__trans phrase="caches" escape="js">'); return false;"
        accesskey="x"
        title="<__trans phrase="Delete selected [_1] (x)" params="<$mt:var name="object_label_plural"$>">"
        ><__trans phrase="Delete"></a>
</mt:setvarblock>
<mtapp:listing>
    <mt:if __first__>
        <thead>
            <tr>
                <th class="cb"><input type="checkbox" name="id-head" value="all" class="select" /></th>
                <th class="cache-key"><__trans phrase="Cache Key"></th>
                <th class="cache-value"><__trans phrase="Value"></th>
                <th class="date"><__trans phrase="Cached date"></th>
            </tr>
        </thead>
        <tbody>
    </mt:if>
            <tr class="<mt:if name="__odd__">odd<mt:else>even</mt:if>">
                <td class="cb"><input type="checkbox" name="id" class="select" value="<mt:var name="cache_key">" /></td>
                <td class="cache-key"><a href="<mt:var name="script_url">?__mode=view&amp;_type=cache&amp;id=<mt:var name="cache_key" escape="html"><mt:if name="blog_id">&amp;blog_id=<mt:var name="blog_id"></mt:if>"><$mt:var name="cache_key"$></a></td>
                <td class="cache_value"><$mt:var name="cache_value" escape="html"$></td>
                <td class="date"><$mt:var name="start_time_formatted"$></td>
            </tr>
    <mt:if __last__>
        </tbody>
    </mt:if>
</mtapp:listing>

