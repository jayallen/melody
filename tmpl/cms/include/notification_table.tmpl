<mtapp:listing>
    <mt:if name="__first__">
    <thead>
        <tr>
            <th class="cb" id="delete-col-head"><input type="checkbox" name="id-head" value="all" class="select" /></th>
            <th id="nt-email"><__trans phrase="Email Address"></th>
            <th id="nt-url"><__trans phrase="URL"></th>
            <th id="nt-date"><__trans phrase="Date Added"></th>
        </tr>
    </thead>
    <tbody>
    </mt:if>
        <tr class="<mt:if name="__odd__">odd<mt:else>even</mt:if>">
            <td class="cb" id="delete-<mt:var name="id">"><input type="checkbox" name="id" value="<mt:var name="id">" class="select" /></td>
            <td><a href="javascript:void(0)" title="<__trans phrase="Click to edit notification list item">" class="edit-link" id="note-email-link-<mt:var name="id">" onmouseover="highlight_note(<mt:var name="id">)" onmouseout="unhighlight_note(<mt:var name="id">)" onclick="return edit_note(<mt:var name="id">)"><mt:var name="email" escape="html"></a>
                <span id="note-email-field-<mt:var name="id">" style="display: none">
                    <input type="text" name="note-email-<mt:var name="id">" id="note-email-<mt:var name="id">" value="<mt:var name="email" escape="html">" />
                </span>
            </td>
            <td><span id="note-url-link-<mt:var name="id">" onmouseover="highlight_note(<mt:var name="id">)" onmouseout="unhighlight_note(<mt:var name="id">)" onclick="return edit_note(<mt:var name="id">)"><a href="javascript:void(0)" title="<__trans phrase="Click to edit notification list item">"><mt:var name="url" escape="html"></a><mt:if name="url">&nbsp;<a href="<mt:var name="url">">&raquo;</a></mt:if></span>
                <span id="note-url-field-<mt:var name="id">" style="display: none">
                    <input type="text" name="note-url-<mt:var name="id">" id="note-url-<mt:var name="id">" value="<mt:var name="url" escape="html">" />
                    <input type="submit" value="<__trans phrase="Save">" onclick="return submit_edit(<mt:var name="id">)" />
                    <input type="button" value="<__trans phrase="Cancel">" onclick="return cancel_edit(<mt:var name="id">)" />
                </span>
            </td>
            <td><span title="<mt:var name="created_on_time_formatted">"><mt:if name="created_on_relative"><mt:if name="dates_relative"><mt:var name="created_on_relative"><mt:else><mt:var name="created_on_formatted"></mt:if><mt:else><mt:var name="created_on_formatted"></mt:if></span></td>
        </tr>
    <mt:if name="__last__">
    </tbody>
    </mt:if>
</mtapp:listing>
