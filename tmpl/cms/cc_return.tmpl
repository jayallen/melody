<script type="text/javascript">
function getByID (d, n) {
    if (d.getElementById)
        return d.getElementById(n);
    else if (d.all)
        return d.all[n];
}
var d = window.opener.document;
d.cfg_form.cc_license.value = '<TMPL_VAR NAME=LICENSE_CODE ESCAPE=HTML>';
var e = getByID(d, 'cc-license-name');
if (e)
    e.innerHTML = '<TMPL_VAR NAME=LICENSE_NAME ESCAPE=HTML>';
if (e = getByID(d, 'no-license'))
    e.style.display = 'none';
if (e = getByID(d, 'has-license'))
    e.style.display = 'block';
window.close();
</script>
